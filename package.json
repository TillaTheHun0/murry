{
  "name": "murry",
  "version": "1.1.0",
  "description": "Separate controller and router with curried marshalling",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build:dev": "npm run build -- -s",
    "build": "rimraf dist && tsc --declaration --outDir dist",
    "bump": "npx standard-version",
    "prepare": "npm run build",
    "standard": "node ./node_modules/standard/bin/cmd.js \"**/*.js\"",
    "test": "npm run standard && nyc ts-mocha test/**/*.spec.ts",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "author": "Tyler Hall",
  "license": "MIT",
  "dependencies": {
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "@types/ramda": "^0.26.9",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "husky": "^2.4.1",
    "mocha": "^6.1.4",
    "nyc": "^14.0.0",
    "rimraf": "^2.6.3",
    "standard": "^12.0.1",
    "standard-version": "^6.0.1",
    "ts-mocha": "^6.0.0",
    "tslint": "^5.17.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.5.2"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": [
      "/docs/**"
    ]
  }
}
