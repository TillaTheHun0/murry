{
  "name": "murry",
  "version": "1.0.0",
  "description": "Separate controller and router with curried marshalling",
  "main": "dist/index.js",
  "scripts": {
    "build:dev": "npm run build -- -s",
    "build": "node ./node_modules/.bin/babel src --out-dir dist",
    "bump": "npx standard-version",
    "prepare": "npm run build",
    "standard": "node ./node_modules/standard/bin/cmd.js \"**/*.js\"",
    "test": "npm run standard && nyc mocha --require @babel/register",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "author": "Tyler Hall",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "@commitlint/cli": "^7.3.2",
    "@commitlint/config-conventional": "^7.3.1",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "husky": "^1.3.1",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "standard": "^12.0.1",
    "standard-version": "^4.4.0"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": [
      "/docs/**"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm test"
    }
  }
}
